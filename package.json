  "scripts": {
    "update-version": "./update_version.sh",
    "build-server": "cd src/api && go build -o ../../dist/",
    "build-client": "webpack --env OFFICEMAP_ENV=development --env platformtype=web --mode development",
    "build-server-qa": "cd src/api && go build -o ../../dist/ -ldflags \"-s -w\"",
    "build-client-qa": "webpack --env OFFICEMAP_ENV=qa --env platformtype=web --mode production",
    "build-server-prod": "cd src/api && go build -o ../../dist/ -ldflags \"-s -w\" -o ../../dist/",
    "build-client-pord": "webpack --env OFFICEMAP_ENV=production --env platformtype=web --mode production",
    "build": "npm run build-server && npm run build-client",
    "build-qa": "npm run build-server-qa && npm run build-client-qa",
    "build-prod": "npm run build-server-prod && npm run build-client-prod"
  }
